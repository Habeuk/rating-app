{
  "name": "rating-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build --debug",
    "preview": "vite preview",
    "build-reviews-dev": "vite build --watch --mode development  --outDir ratingReviews -c vite-reviews.config.js",
    "build-reviews-prod": "vite build --mode production  --outDir ratingReviews -c vite-reviews.config.js",
    "build-stars-dev": "vite build --watch --mode development  --outDir StarsApp -c vite-stars.config.js",
    "build-stars-prod": "vite build --mode production  --outDir StarsApp -c vite-stars.config.js",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "help": "vite build --help",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@stephane888/wbu-atomique": "file:../wbu-atomique",
    "axios": "^1.4.0",
    "drupal-vuejs": "file:../drupal-vuejs",
    "primevue": "^3.36.0",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "vite": "^4.3.9"
  }
}
